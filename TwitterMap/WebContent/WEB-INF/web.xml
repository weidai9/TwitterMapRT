<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:web="http://xmlns.jcp.org/xml/ns/javaee">
  <servlet>
    <display-name>WorkerServlet</display-name>
    <servlet-name>WorkerServlet</servlet-name>
    <servlet-class>tmpkg.WorkerServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Main</servlet-name>
    <jsp-file>/main.jsp</jsp-file>
  </servlet>
  <servlet>
    <display-name>EchoServer</display-name>
    <servlet-name>EchoServer</servlet-name>
    <servlet-class>tmpkg.EchoServer</servlet-class>
  </servlet>
  <servlet>
    <display-name>MapUpdateServer</display-name>
    <servlet-name>MapUpdateServer</servlet-name>
    <servlet-class>tmpkg.MapUpdateServer</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>WorkerServlet</servlet-name>
    <url-pattern>/WorkerServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Main</servlet-name>
    <url-pattern>/main</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>EchoServer</servlet-name>
    <url-pattern>/echoserverlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>MapUpdateServer</servlet-name>
    <url-pattern>/mapupdateserver</url-pattern>
  </servlet-mapping>
</web-app>